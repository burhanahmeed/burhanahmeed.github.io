    "def rating_function(x):\n",
    "    if x >= 8.0:\n",
    "        return \"good\"\n",
    "    else:\n",
    "        return \"bad\"\n",
    "    \n",
    "movies_df[\"rating_category\"] = movies_df[\"rating\"].apply(rating_function)\n",
